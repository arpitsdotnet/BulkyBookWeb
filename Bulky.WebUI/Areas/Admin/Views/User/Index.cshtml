
@model List<ApplicationUser>

<div class="container">
    <div class="row pt-4 pb-3">
        <div class="col-6">
            <h2 class="text-primary">Usar List </h2>
        </div>
    </div>
</div>

<table id="tblData" class="table table-bordered table-dark table-sm table-striped table-hover text-nowrap" style="width:100%;">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Company</th>
            <th>Role</th>
            <th>Actions</th>
        </tr>
    </thead>
</table>

@section Scripts {
    <script type="text/javascript" src="~/js/user.js"></script>
}